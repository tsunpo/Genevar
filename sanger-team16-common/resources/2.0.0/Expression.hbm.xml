<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sanger.team16.common.hbm.Expression" table="expression">

        <id name="id" column="expression_id">
            <generator class="native"/>
        </id>

        <property name="expressionFeatureId" column="population_expression_id"/>
        <property name="transcriptId" column="transcript_id"/>
        <property name="individualId" column="individual_id"/>
        <property name="value" column="value"/>
        
    </class>
</hibernate-mapping>
