<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sanger.team16.common.hbm.Genotype" table="genotype">

        <id name="id" column="genotype_id">
            <generator class="native"/>
        </id>
  
        <property name="genotypeFeatureId" column="population_genotype_id"/>
        <property name="variationId" column="variation_id"/>
        <property name="individualId" column="individual_id"/>
        <property name="value" column="value"/>
        
    </class>
</hibernate-mapping>
